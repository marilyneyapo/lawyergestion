{% extends 'base.html.twig' %}

{% block title %}Cabinet{% endblock %}

{% block body %}
    <h1>Cabinet</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ cabinet.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ cabinet.nom }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ cabinet.adresse }}</td>
            </tr>
            <tr>
                <th>Codepostal</th>
                <td>{{ cabinet.codepostal }}</td>
            </tr>
            <tr>
                <th>Tel</th>
                <td>{{ cabinet.tel }}</td>
            </tr>
            <tr>
                <th>Fax</th>
                <td>{{ cabinet.fax }}</td>
            </tr>
            <tr>
                <th>Logo</th>
                <td>{{ cabinet.logo }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_cabinet_index') }}">back to list</a>

    <a href="{{ path('app_cabinet_edit', {'id': cabinet.id}) }}">edit</a>

    {{ include('cabinet/_delete_form.html.twig') }}
{% endblock %}
